{
  "name": "mapea",
  "version": "6.6.0",
  "description": "Herramienta para desarrollo de mapas interactivos de forma sencilla",
  "keywords": [
    "map",
    "mapping",
    "tool",
    "sigcorporativo-ja"
  ],
  "homepage": "http://mapea4-sigc.juntadeandalucia.es/",
  "repository": {
    "type": "git",
    "url": "https://github.com/sigcorporativo-ja/Mapea4"
  },
  "license": "EUPL",
  "bugs": {
    "url": "https://github.com/sigcorporativo-ja/Mapea4/issues"
  },
  "devDependencies": {
    "chromedriver": "^2.46.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "6.8.1",
    "css-minimizer-webpack-plugin": "5.0.1",
    "eslint": "8.51.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-webpack-plugin": "^4.0.1",
    "expect.js": "0.3.1",
    "expose-loader": "^0.7.5",
    "file-loader": "^1.1.11",
    "fs-extra": "2.0.0",
    "get-image-colors": "^4.0.1",
    "html-loader": "^0.5.5",
    "jsdoc-plugin-typescript": "^2.2.0",
    "karma": "^6.3.8",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "2.0.1",
    "karma-webpack": "^3.0.5",
    "mini-css-extract-plugin": "2.7.6",
    "mocha": "10.2.0",
    "nightwatch": "^2.6.13",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "style-loader": "3.3.3",
    "terser-webpack-plugin": "5.3.9",
    "uglify-js": "^2.6.1",
    "uglifycss": "0.0.29",
    "walk": "^2.3.9",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "yargs": "17.0.0"
  },
  "dependencies": {
    "chroma-js": "^1.3.7",
    "draggabilly": "3.0.0",
    "handlebars": "4.5.2",
    "jsdoc": "^3.6.4",
    "jsts": "^2.0.2",
    "ol": "8.1",
    "proj4": "2.8.1"
  },
  "overrides": {
    "webpack": "5.88.2",
    "eslint": "8.51.0"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=2048 ./node/npm run build:core && ./node/npm run build:plugins && ./node/npm run build:plugins-extern",
    "build:sourcemap": "npm run prebuild && npm run build:core -- --source-map && npm run build:plugins",
    "build:core": "webpack --config=webpack-config/webpack.production-core.config.js",
    "build:plugins": "webpack --config=webpack-config/webpack.production-plugins.config.js",
    "build:plugins-extern": "./node/node tasks/build-extern-plugins.js",
    "debug:build": "webpack serve --config=webpack-config/webpack.test-production.config.js",
    "debug:prebuild": "node --inspect-brk tasks/generate-index.js",
    "generate-doc": "jsdoc config/jsdoc/api/index.md -c config/jsdoc/api/conf.json",
    "generate-test": "node tasks/test-generator",
    "karma": "karma start ./karma.conf.js",
    "nightwatch": "nightwatch --config=./nightwatch.conf.js",
    "prebuild": "node tasks/clean-build.js --path=dist && node tasks/generate-index.js && node tasks/generate-plugins-entrypoints.js",
    "postbuild": "npm run generate-doc && node tasks/copy-legacy.js",
    "publish_npm": "node tasks/prepare-package/prepare.js",
    "start": "webpack serve --config=webpack-config/webpack.development.config.js",
    "start:build": "npm run build && npm run debug:build",
    "test": "npm run karma && npm run nightwatch"
  }
}
